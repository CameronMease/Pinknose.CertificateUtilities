<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Copyright>Copyright (c) 2021 Cameron Mease</Copyright>
    <PackageProjectUrl>https://github.com/CameronMease/Pinknose.CertificateUtilities</PackageProjectUrl>
    <RepositoryUrl>https://github.com/CameronMease/Pinknose.CertificateUtilities</RepositoryUrl>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <Authors></Authors>
    <Description>Utilities to help in building and managing cryptographic certificates.</Description>
    <PackageReadmeFile>README.md</PackageReadmeFile>
	  <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

	<PropertyGroup>
		<Version>0.0.1</Version>
		<Authors>cameronmease</Authors>
	</PropertyGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
